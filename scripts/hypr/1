# Ваша строка
str="Volume: 1.01"

# Извлечение числа из строки
num=$(echo $str | grep -o -E '[0-9]+\.[0-9]+')

# Умножение числа на 100
num100=$(echo $num | awk '{print $1 * 100}')

# Ваше другое число
x=96

# Сравнение чисел
if awk 'BEGIN {exit !('$x' < '$num100')}'; then
  echo "Число $x меньше, чем $num умноженное на 100"
else
  echo "Число $x не меньше, чем $num умноженное на 100"
fi

